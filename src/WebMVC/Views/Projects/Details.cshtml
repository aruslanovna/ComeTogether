    <style>
        .container {
            position: relative;
           
            height: 100vh;
        }

           

        .header {
            position: relative;
            z-index: 2;
            display: -webkit-box;
            display: flex;
            -webkit-box-pack: justify;
            justify-content: space-between;
            -webkit-box-align: center;
            align-items: center;
            width: 100%;
            height: 10%;
            padding: 0 2rem;
        }

        .header-logo {
            text-transform: uppercase;
            color: #493330;
        }

        .header-nav {
            display: -webkit-box;
            display: flex;
            -webkit-box-align: center;
            align-items: center;
            width: auto;
            height: 100%;
            line-height: 1.2rem;
        }

            .header-nav i {
                font-size: 1rem;
            }

            .header-nav div {
                position: relative;
                width: 1.5rem;
                height: 2px;
                background: #493330;
                margin-left: 2rem;
            }

                .header-nav div:before {
                    position: absolute;
                    content: '';
                    display: block;
                    top: -4px;
                    right: 0;
                    width: 120%;
                    height: 2px;
                    background: #493330;
                }

                .header-nav div:after {
                    position: absolute;
                    content: '';
                    display: block;
                    bottom: -4px;
                    right: 0;
                    width: 50%;
                    height: 2px;
                    background: #493330;
                }

        .product {
            position: relative;
            display: -webkit-box;
            display: flex;
            width: 100%;
            height: 90%;
            z-index: 2;
            padding: 0 2rem;
        }

        .product-photo {
            display: -webkit-box;
            display: flex;
            -webkit-box-pack: justify;
            justify-content: space-between;
            -webkit-box-align: center;
            align-items: center;
            width: 52%;
            height: 100%;
        }

            .product-photo img:first-child {
                width: 34%;
                height: auto;
                margin-top: 10%;
                -webkit-transform: translate(0, 0);
                transform: translate(0, 0);
                -webkit-animation-name: small;
                animation-name: small;
                -webkit-animation-duration: 6s;
                animation-duration: 6s;
                -webkit-animation-iteration-count: infinite;
                animation-iteration-count: infinite;
                -webkit-animation-timing-function: ease-in-out;
                animation-timing-function: ease-in-out;
            }

            .product-photo img:last-child {
                width: 50%;
                height: atuo;
                -webkit-animation-name: big;
                animation-name: big;
                -webkit-animation-duration: 6s;
                animation-duration: 6s;
                -webkit-animation-iteration-count: infinite;
                animation-iteration-count: infinite;
                -webkit-animation-timing-function: ease-in-out;
                animation-timing-function: ease-in-out;
                animation-direction: reverse;
            }

        .product-detail {
            display: -webkit-box;
            display: flex;
            -webkit-box-orient: vertical;
            -webkit-box-direction: normal;
            flex-flow: column;
            -webkit-box-pack: center;
            justify-content: center;
            -webkit-box-align: center;
            align-items: center;
            width: 48%;
            height: 100%;
            text-align: center;
            padding: 0 5%;
        }

        .product__title {
            font-size: 1.8rem;
            margin-bottom: 2rem;
        }

        .product__price {
            letter-spacing: 2px;
            font-family: "Montserrat", sans-serif;
            margin-bottom: 2rem;
        }

        .product__subtitle {
            font-family: "Montserrat", sans-serif;
            font-weight: 300;
            font-size: .75rem;
            color: #B9B8B6;
            margin-bottom: 2rem;
            padding: 0 4rem;
        }

        .product__color form {
            display: -webkit-box;
            display: flex;
        }

            .product__color form fieldset {
                position: relative;
                width: 1.5rem;
                height: 1.5rem;
            }

                .product__color form fieldset label {
                    display: block;
                    width: 100%;
                    height: 100%;
                    border-radius: 100%;
                    border: 0;
                    line-height: 1.6rem;
                }

                    .product__color form fieldset label i {
                        opacity: 0;
                        -webkit-transition: .32s;
                        transition: .32s;
                    }

                .product__color form fieldset input {
                    position: absolute;
                    z-index: 3;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    opacity: 0;
                    cursor: pointer;
                }

                    .product__color form fieldset input:checked + label i {
                        opacity: 1;
                    }

                .product__color form fieldset:first-child {
                    margin-right: 1rem;
                }

                    .product__color form fieldset:first-child label {
                        background: #B9B8B6;
                    }

                        .product__color form fieldset:first-child label i {
                            color: #513F35;
                        }

                .product__color form fieldset:last-child label {
                    background: #513F35;
                }

                    .product__color form fieldset:last-child label i {
                        color: #B9B8B6;
                    }

        .product__button {
            position: relative;
            overflow: hidden;
            margin-top: 2rem;
            text-decoration: none;
            text-transform: uppercase;
            border-radius: 4px;
            background: #F1EDE4;
            display: block;
            width: 8rem;
            height: 2rem;
            font-size: .75rem;
            font-family: "Montserrat", sans-serif;
            color: #513F35;
            -webkit-transition: .32s;
            transition: .32s;
        }

            .product__button span {
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                line-height: 2rem;
                -webkit-transition: .32s;
                transition: .32s;
            }

                .product__button span:last-child {
                    top: -4rem;
                    -webkit-transition-delay: .32s;
                    transition-delay: .32s;
                    color: #fff;
                }

        .product__button--success {
            background: #00ff7f;
        }

            .product__button--success span:last-child {
                top: 0;
            }

            .product__button--success span:first-child {
                top: -4rem;
            }
    </style>
<script>
    const button = document.querySelector('.product__button');

    function buttonAnimate() {
        button.classList.add('product__button--success');
    }
</script>

